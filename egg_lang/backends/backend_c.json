{
    "file_generation": [
        {
            "comment": "Used for generating header files",
            "type": "File",
            "name_ops": [
                {
                    "type": "FileName"
                },
                {
                    "type": "Concat",
                    "value": ".h"
                }
            ],
            "content_ops": [
                {
                    "type": "FunctionDefinitions"
                },
                {
                    "type": "Concat",
                    "value": "\n"
                },
                {
                    "type": "StructDefinitions"
                }
            ]
        },
        {
            "comment": "Used for generating implementation files",
            "type": "File",
            "name_ops": [
                {
                    "type": "FileName"
                },
                {
                    "type": "Concat",
                    "value": ".c"
                }
            ],
            "content_ops": [
                {
                    "type": "FunctionImplementations"
                }
            ]
        }
    ],
    "func_rules": {
        "implementation_ops": [
            {
                "type": "ReturnType"
            },
            {
                "type": "Concat",
                "value": " "
            },
            {
                "type": "FuncName"
            },
            {
                "type": "Concat",
                "value": "("
            },
            {
                "type": "BuildProperties"
            },
            {
                "type": "Concat",
                "value": ") {\n"
            },
            {
                "type": "Concat",
                "value": "}\n"
            }
        ],
        "property_ops": [
            {
                "type": "PropertyType"
            },
            {
                "type": "Concat",
                "value": " "
            },
            {
                "type": "PropertyName"
            },
            {
                "type": "ConcatIfNotEnd",
                "value": ", "
            }
        ],
        "declaration_ops": [
            {
                "type": "ReturnType"
            },
            {
                "type": "Concat",
                "value": " "
            },
            {
                "type": "FuncName"
            },
            {
                "type": "Concat",
                "value": "("
            },
            {
                "type": "BuildProperties"
            },
            {
                "type": "Concat",
                "value": ");\n"
            }
        ]
    },
    "struct_rules": {
        "ops": [
            {
                "type": "Concat",
                "value": "struct "
            },
            {
                "type": "StructName"
            },
            {
                "type": "Concat",
                "value": " {\n"
            },
            {
                "type": "BuildProperties"
            },
            {
                "type": "Concat",
                "value": "} "
            },
            {
                "type": "StructName"
            },
            {
                "type": "Concat",
                "value": ";"
            }
        ],
        "property_ops": [
            {
                "type": "Concat",
                "value": "\t"
            },
            {
                "type": "PropertyType"
            },
            {
                "type": "Concat",
                "value": " "
            },
            {
                "type": "PropertyName"
            },
            {
                "type": "Concat",
                "value": ";\n"
            }
        ]
    }
}